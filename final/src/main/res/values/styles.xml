<resources>
    <style name="AppTheme" parent="Theme.AppCompat.DayNight">
		<item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
		<item name="textPrimary">@color/textPrimary</item>
		<item name="textSecondary">@color/textSecondary</item>
		<item name="titleColor">#fffafafa</item>
		<item name="actionStyle">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
		<item name="popupStyle">@style/ThemeOverlay.AppCompat.Light</item>
		<item name="android:textColor">#444444</item>
		<item name="btnColor">#DCDCDC</item>
	</style>
	
